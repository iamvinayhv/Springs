package com.vinay;

import java.util.List;

public class Question {

	private String id;
	private String question;
	private List<Answers> answers;
	
	
	
	public Question(String id, String question, List<Answers> answers) {

		this.id = id;
		this.question = question;
		this.answers = answers;
	}
	
	
	
}
