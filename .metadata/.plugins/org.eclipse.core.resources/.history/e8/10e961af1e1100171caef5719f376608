package com.vinay;

public class CarInsurence implements Insurence {

	private String name;
	private Validity va;
	
	
	
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void status()
	{
		System.out.println(name+" your Car Insured");
		
		System.out.println(va.getValidity());
		
	}

	public CarInsurence(Validity va) {
		super();
		this.va = va;
	}

	public String offers() {
		
		return "You have 10% dic. offer in next Shopping";
	}

	
	
	
	

}
