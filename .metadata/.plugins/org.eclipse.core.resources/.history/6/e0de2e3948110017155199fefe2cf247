package com.vinay;

import java.util.Map;

public class Question {

	private int id;
	private String question;
	private Map<String,String> answer;
	
	
	public Question(int id, String question, Map<String, String> answer) {
		super();
		this.id = id;
		this.question = question;
		this.answer = answer;
	}
	
	
	
}
