package com.bridgelabz.model;

public class DemoA {

	private DemoB b;
	
	public DemoA() {

		System.out.println("DemoA created");
	}

	public DemoB getB() {
		return b;
	}

	public void setB(DemoB b) {
		this.b = b;
	}
	
	void print() {
		System.out.println("hi a");
	}
	
	public void disp()
	{
		print();
		b.print();
	}
	
}
