package com.vinay;

import org.springframework.beans.factory.BeanFactory;

public class Student {

	private String name;
	private int age;
	
	
	public String getName() {
		return name;
	}

	

	public int getAge() {
		return age;
	}

	
	public void display(){  
	    System.out.println("Hello: "+name+" age:"+age);  
	}  	
	
}
