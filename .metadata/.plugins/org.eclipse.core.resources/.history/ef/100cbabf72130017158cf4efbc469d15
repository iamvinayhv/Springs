package com.bridgelabz.ByType.model;

public class Employee {

	private int id;
	private String name;
	
	private Address address;

	public Employee(Address address) {
	}


	
	public void disp() {
		System.out.println("Id:"+id+", Name:"+name);
		System.out.println(address.getCity()+","+address.getCountry());
	}
	
}
